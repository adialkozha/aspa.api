@model IEnumerable<AspaApp.Models.User>
@{
    if (User.Identity.IsAuthenticated == true)
    {

        Layout = "_Layout2";
    }
    else
    {

        Layout = "_Layout";
    }


    ViewData["Title"] = "Admin page";
}
<h1>@ViewData["Title"]</h1>
<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Create">Добавить товар</a>
<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Adminproduct">Удалить товар</a>
<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Userlist">Удалить пользователя</a>
@foreach (var u1 in Model.Where(p => p.Email == User.Identity.Name))
{


    <h4>Id : @u1.Id</h4>
    <h4>Email: @u1.Email</h4>
    <h4>Role Id: @u1.RoleId</h4>
    



}
